server {
    listen 80;
    server_name 64.225.79.119;
    client_max_body_size 4m;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
       root /home/root/ddfood_backend;
    }

    location /media/ {
       root /home/root/ddfood_backend;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/root/ddfood_backend/ddfood_backend.sock;
    }
}
